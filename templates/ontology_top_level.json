{{'''
    SELECT distinct ?class WHERE {
    graph ?g {
      ?target a owl:Ontology.
      ?c a owl:Class.
    }
    ?c rdfs:subClassOf* ?class.
    FILTER NOT EXISTS {?class rdfs:subClassOf ?superClass.}
    FILTER (!isBlank(?class))
  } 
''' | query(values={"target":this.identifier}) | iter_labelize(key="class", label_key="label") | tojson }}
