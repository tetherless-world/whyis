{{'''
    SELECT distinct ?class WHERE {
    graph ?g {
      ?target a owl:Ontology.
      ?c a owl:Class.
    }
    ?c rdfs:subClassOf* ?class.
    FILTER(!isBlank(?class)).
  #  minus { ?class rdfs:subClassOf ?superClass.}
  
    FILTER(
        !EXISTS {
         ?class rdfs:subClassOf ?superClass.
      }
    )
  } 
''' | query(values={"e":this.identifier}) | iter_labelize(key="class", label_key="label") | tojson }}
