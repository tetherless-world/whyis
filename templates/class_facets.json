{{'''SELECT distinct ?class ?property ?propertyType ?inverse ?facetId WHERE {
  ?s a ?class.
  ?s ?property ?o.
  ?property a ?propertyType.
  optional {
      ?property rdfs:range ?range.
  }
  bind(?property as ?facetId)
}
LIMIT 100'''|query(values={"class":this.identifier}) | iter_labelize('property','label') | map_list('property','facetId',g.slugify,separator='_') | tojson }}
