{{'''
  SELECT ?identifier (sample(?d) as ?description) (max(?s) as ?score) (sample(?label) as ?text)
  WHERE {
      SERVICE <http://aws.amazon.com/neptune/vocab/v01/services/fts> {
          [] fts:search ?query ;
             fts:matchQuery "*" ;
             fts:entity ?identifier ;
             fts:score ?s .
      }
      ?identifier ?p ?label .
      filter(!isBlank(?identifier))
      filter(lang(?label) = "" || langMatches(lang(?label), "en"))
      OPTIONAL {
          ?identifier dc:description|skos:definition|rdfs:comment|sioc:content|dc:abstract|dc:summary|rdfs:comment|dcelements:description|<http://purl.obolibrary.org/obo/IAO_0000115>|prov:value|sio:hasValue|<http://dbpedia.org/ontology/abstract> ?d.
          filter(lang(?d) = "" || langMatches(lang(?d), "en"))
      }
  } group by ?identifier having max(?s)
  ORDER BY DESC(?score)
  LIMIT 1000''' | query(values={"query":rdflib.Literal(args['query'])}) | iter_labelize("identifier","label") | tojson }}
