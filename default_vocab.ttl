  rdfs:subPropertyOf whyis:hasView.

whyis:hasLabel dc:identifier "label";
  rdfs:subPropertyOf whyis:hasView.

whyis:hasSummary dc:identifier "summary";
  rdfs:subPropertyOf whyis:hasView.

